    </servlet-mapping>


    <servlet>
        <servlet-name>other</servlet-name>
        <servlet-class>WebServer2.OtherServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>other</servlet-name>
        <url-patten>/o</url-patten>
    </servlet-mapping>
